for i in `seq 1 20`
do 
  make DEFINE_FLAGS="-DSEC_SIZE=128 -DGALOIS_FIELD_EXT=11 -DNO_OF_ERRORS=$i"; make SZ=128 NO=$i cr;
  make DEFINE_FLAGS="-DSEC_SIZE=256 -DGALOIS_FIELD_EXT=12 -DNO_OF_ERRORS=$i"; make SZ=256 NO=$i cr;
  make DEFINE_FLAGS="-DSEC_SIZE=512 -DGALOIS_FIELD_EXT=13 -DNO_OF_ERRORS=$i"; make SZ=512 NO=$i cr;
done


